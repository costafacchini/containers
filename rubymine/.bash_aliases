alias rubymine="docker start rubymine || docker run -d --log-driver=none --net host -it --add-host db:127.0.0.1 -v /etc/localtime:/etc/localtime -v /tmp/.X11-unix:/tmp/.X11-unix -v ~/.RubyMine2016.1:/root/.RubyMine2016.1 -v ~/.RubyMine2016.3:/root/.RubyMine2016.3 -v ~/projects:/Projects -e DISPLAY=unix$DISPLAY -e LANG=C.UTF-8 --name rubymine rubymine"
